<h1>My friends</h1>


<table class "table">
 <thead class="thead thead-default">
  <tr>
   <th>Name</th>
   <th>Email</th>
   <th>Friend since</th>
  </tr>
 </thead>

 <tbody>
  <% @friends.each do |friend| %>
   <tr>
    <td><%= friend.name %></td>
    <td><%= friend.email %></td>   
    <td><%= time_ago_in_words friend.created_at %></td>
   </tr>
  <% end %>
 </tbody>
</table>



