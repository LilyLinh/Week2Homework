<h1>Send Message</h1>

<table class "table">
 <thead class="thead thead-default">

  <tr>
   <th>Sender ID</th>
   <th>Created at</th>
  </tr>

 </thead>

 <tbody>
  <% @messages.each do |message| %>
   <tr>
    <td><%= message.sender_id %></td>  
    <td><%= time_ago_in_words message.created_at %></td>

    
   </tr>
  <% end %>
 </tbody>
</table>
