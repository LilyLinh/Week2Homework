<h1>Tat ca cac message</h1>


<table class "table">
 <thead class="thead thead-default">
  <tr>
   <th>Sender ID</th>
   <th>Receiver ID</th>
   <th>content</th>
   <th>Created at</th>
  </tr>
 </thead>
 <tbody>
  <% @messages.each do |message| %>
   <tr>
    <td><%= message.sender_id %></td>
    <td><%= message.receiver_id %></td>
    <td><%= message.content %></td>    
    <td><%= time_ago_in_words message.created_at %></td>
   </tr>
  <% end %>
 </tbody>
</table>

